{
	"name": "thepercival/vobetting",
	"description": "website for football betting and analytics",
	"keywords": ["games","football","betting","analytics"],
	"homepage": "https://www.vobetting.nl/",
	"license": "GPL-2.0",
	"authors": [
		{
			"name": "Coen Dunnink",
			"email": "coendunnink@gmail.com"
		}
	],
	"support": {
		"email": "coendunnink@gmail.com"
	},
	"require": {
		"erlangb/betfair": "^0.2",
		"thepercival/voetbal": "dev-master",
		"php": ">=5.6.0",
		"slim/slim": "^3.1",
		"slim/php-view": "^2.0",
		"monolog/monolog": "^1.17",
		"doctrine/orm": "^2.5",
		"tuupola/slim-jwt-auth": "^2.3",
		"vlucas/phpdotenv": "^2.4",
		"firebase/php-jwt": "^4.0",
		"symfony/property-access": "^3.1",
		"phpmailer/phpmailer": "^5.2",
		"jms/serializer": "^1.5",
		"symfony/yaml": "^3.2"
	},
	"require-dev": {
		"phpunit/phpunit": "~5.7"
	},
	"autoload": {
		"psr-4": {
			"VOBetting\\": "domain/",
			"VOBettingRepository\\": "db/repositories",
			"App\\Action\\": "app/actions"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"VOBetting\\Tests\\": "tests/"
		}
	},
	"minimum-stability": "dev",
	"scripts": {
		"start": "php -S localhost:2999 -t public public/index.php",
		"test": [
			"phpunit -c tests/config.xml"
		]
	}
}